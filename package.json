{
  "name": "users-policies-assessment",
  "version": "1.0.0",
  "description": "Policies API",
  "scripts": {
    "start": "node dist",
    "prestart": "yarn build",
    "lint": "eslint src test",
    "codecov": "cat coverage/*/lcov.info | codecov",
    "build": "rimraf dist && babel src --copy-files --out-dir dist --source-maps",
    "start:dev": "nodemon --watch .env --watch src --exec babel-node src --source-maps",
    "test": "cross-env NODE_ENV=test mocha --exit --recursive --require @babel/register test",
    "test:coverage": "cross-env NODE_ENV=test nyc --require @babel/register --reporter lcov --reporter text mocha --exit --recursive test"
  },
  "keywords": [
    "javascript",
    "node",
    "express",
    "es6"
  ],
  "private": true,
  "author": "Adrian Rodriguez <mailto:karodriguezr@unal.edu.co>",
  "dependencies": {
    "@hapi/boom": "^9.1.0",
    "body-parser": "^1.19.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-validator": "^6.5.0",
    "helmet": "^3.21.3",
    "http-status-codes": "^1.4.0",
    "morgan": "^1.10.0",
    "swagger-jsdoc": "^3.5.0",
    "swagger-ui-dist": "^3.25.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/preset-env": "^7.9.0",
    "@babel/register": "^7.9.0",
    "babel-eslint": "^10.1.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "chai": "^4.2.0",
    "codecov": "^3.6.5",
    "cross-env": "^7.0.2",
    "eslint": "^6.8.0",
    "eslint-plugin-flowtype": "^5.1.3",
    "mocha": "^7.1.1",
    "nodemon": "^2.0.2",
    "nyc": "^15.0.0",
    "supertest": "^4.0.2"
  },
  "engines": {
    "node": ">= 10.13.0",
    "npm": ">= 6.4.1",
    "yarn": ">= 1.20.0"
  }
}
